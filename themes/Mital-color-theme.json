{
	"name": "Mital",
	"colors": {
	  "editor.background": "#F5FAFF",
	  "editor.foreground": "#2C3E50",
	  "list.activeSelectionIconForeground": "#555555",
	  "activityBar.activeBackground": "#E8EEF3",
	  "sideBar.background": "#F9F9F9",
	  "sideBar.border": "#EBEEF0",
	  "sideBar.foreground": "#2C3E50",
	  "sideBar.dropBackground": "#F9F9F9",
	  "sideBarTitle.border": "#AAB2B9",
	  "sideBarSectionHeader.background": "#ECECEC",
	  "sideBarSectionHeader.border": "#F2F2F2",
	  "activityBar.background": "#F9F9F9",
	  "activityBar.activeBorder": "#34495E",
	  "activityBar.activeFocusBorder": "#34495E",
	  "activityBar.foreground": "#34495E",
	  "banner.background": "#F9F9F9",
	  "breadcrumb.background": "#F5FAFF",
	  "titleBar.activeBackground": "#F9F9F9",
	  "tab.activeBackground": "#F5FAFF",
	  "tab.hoverBackground": "#F9F9F9",
	  "tab.activeBorder": "#34495E",
	  "tab.inactiveBackground": "#F9F9F9"
	},
	"tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6e7677"
      }
    },
    {
      "name": "Comments: Preprocessor",
      "scope": "comment.block.preprocessor",
      "settings": {
        "foreground": "#6e7677"
      }
    },
    {
      "name": "Comments: Documentation",
      "scope": [
        "comment.documentation",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#6e7677",
      }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#C0392B"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#34495E"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#704080",
      }
    },
    {
      "name": "Types",
      "scope": [
        "storage.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#704080"
      }
    },
    {
      "name": "Language Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#D35400"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable"
      ],
      "settings": {
        "foreground": "#16A085"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#808000",
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": {
        "foreground": "#2980B9",
      }
    },
    {
      "name": "Exceptions",
      "scope": "entity.name.exception",
      "settings": {
        "foreground": "#C0392B"
      }
    },
    {
      "name": "Sections",
      "scope": "entity.name.section",
      "settings": {
        "foreground": "#2980B9",
      }
    },
    {
      "name": "Numbers, Characters",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant"
      ],
      "settings": {
        "foreground": "#27AE60"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#2980B9"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#E74C3C"
      }
    },
    {
      "name": "Strings: Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#D35400"
      }
    },
    {
      "name": "Strings: Symbols",
      "scope": "constant.other.symbol",
      "settings": {
        "foreground": "#8E44AD"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#2C3E50"
      }
    },
    {
      "name": "HTML: Doctype Declaration",
      "scope": [
        "meta.tag.sgml.doctype",
        "meta.tag.sgml.doctype string",
        "meta.tag.sgml.doctype entity.name.tag",
        "meta.tag.sgml punctuation.definition.tag.html"
      ],
      "settings": {
        "foreground": "#7F8C8D"
      }
    },
    {
      "name": "HTML: Tags",
      "scope": [
        "meta.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#2980B9"
      }
    },
    {
      "name": "HTML: Tag Names",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#2980B9"
      }
    },
    {
      "name": "HTML: Attribute Names",
      "scope": [
        "meta.tag entity.other.attribute-name",
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#16A085",
        "fontStyle": "italic"
      }
    },
    {
      "name": "HTML: Entities",
      "scope": [
        "constant.character.entity",
        "punctuation.definition.entity"
      ],
      "settings": {
        "foreground": "#D35400"
      }
    },
    {
      "name": "CSS: Selectors",
      "scope": [
        "meta.selector",
        "meta.selector entity",
        "meta.selector entity punctuation",
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#8E44AD"
      }
    },
    {
      "name": "CSS: Property Names",
      "scope": [
        "meta.property-name",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#16A085"
      }
    },
    {
      "name": "CSS: Property Values",
      "scope": [
        "meta.property-value",
        "meta.property-value constant.other",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#D35400"
      }
    },
    {
      "name": "CSS: Important Keyword",
      "scope": "keyword.other.important",
      "settings": {
        "foreground": "#C0392B",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#27AE60"
      }
    },
    {
      "name": "Markup: Deletion",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#C0392B"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#E67E22",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup: Error",
      "scope": "markup.error",
      "settings": {
        "foreground": "#C0392B"
      }
    },
    {
      "name": "Markup: Insertion",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#16A085"
      }
    },
    {
      "name": "Markup: Link",
      "scope": "meta.link",
      "settings": {
        "foreground": "#2980B9"
      }
    },
    {
      "name": "Markup: Output",
      "scope": [
        "markup.output",
        "markup.raw"
      ],
      "settings": {
        "foreground": "#2C3E50"
      }
    },
    {
      "name": "Markup: Prompt",
      "scope": "markup.prompt",
      "settings": {
        "foreground": "#2C3E50"
      }
    },
    {
      "name": "Markup: Heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#2980B9"
      }
    },
    {
      "name": "Markup: Strong",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#8E44AD",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Traceback",
      "scope": "markup.traceback",
      "settings": {
        "foreground": "#C0392B"
      }
    },
    {
      "name": "Markup: Underline",
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#27AE60"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#16A085"
      }
    },
    {
      "name": "Markup Styling",
      "scope": [
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#E67E22"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#8E44AD"
      }
    },
    {
      "name": "Extra: Diff Range",
      "scope": [
        "meta.diff.range",
        "meta.diff.index",
        "meta.separator"
      ],
      "settings": {
        "foreground": "#7F8C8D"
      }
    },
    {
      "name": "Extra: Diff From",
      "scope": "meta.diff.header.from-file",
      "settings": {
        "foreground": "#7F8C8D"
      }
    },
    {
      "name": "Extra: Diff To",
      "scope": "meta.diff.header.to-file",
      "settings": {
        "foreground": "#7F8C8D"
      }
    }
  ]
  }
  